{
  "extends": "../tsconfig.json",
  "include": [
    "src/**/*",
//    "src/**/*.ts",
//    "src/**/*.d.ts",
//    "src/**/*.tsx",
//    "src/**/*.vue"
  ],
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig to read more about this file */
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM"],
    "skipLibCheck": true,
  },
}
